.feature {
  display: flex;
  flex-direction: column;
  width: 90%;
  height: 550rpx;
  margin: 24px auto;
  background-color:blanchedalmond;
  border-radius: 22rpx;
  text-align: center;
}
.feature-image{
  margin: 22rpx auto;
  width: 90%;
  height: 70%;
  background-color: beige;
}
.feature-item{
  width: 90%;
  height: 25%;
  margin: 0 auto;
  display: flex;
  text-align: left;  
  flex-direction: column;
}
.feature-title{
  font-size: 35rpx;
}
.feature-describe{
  font-size: 22rpx;
}

.display-type{
  margin-top: 35rpx;
  margin-right: 35rpx;
  display: flex;
  flex-direction: row-reverse;
}

@font-face {
  font-family: "iconfont"; /* Project id 4119519 */
  src: url('//at.alicdn.com/t/c/font_4119519_c66ki2spr2.woff2?t=1686625969310') format('woff2'),
       url('//at.alicdn.com/t/c/font_4119519_c66ki2spr2.woff?t=1686625969310') format('woff'),
       url('//at.alicdn.com/t/c/font_4119519_c66ki2spr2.ttf?t=1686625969310') format('truetype');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 45rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-list:before {
  content: "\e7f6";
}

.icon-morentubiao:before {
  content: "\e7f7";
}

